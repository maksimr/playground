{
  "name": "playground",
  "version": "0.0.0",
  "main": "./src/index.js",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "test": "jest --passWithNoTests",
    "lint": "eslint src/"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/eslint-parser": "7.11.5",
    "eslint": "7.11.0",
    "parcel-bundler": "1.12.4",
    "jest": "26.5.3",
    "jest-cli": "26.5.3"
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-undef": "warn",
      "no-unused-vars": "warn",
      "valid-jsdoc": [
        "warn",
        {
          "requireReturn": false,
          "requireParamDescription": false,
          "requireReturnDescription": false
        }
      ]
    }
  }
}
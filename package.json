{
  "name": "playground",
  "version": "0.0.0",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack-dev-server -d --disable-host-check --history-api-fallback --host 0.0.0.0 --public $SANDBOX_URL",
    "build": "webpack -p",
    "test": "karma start",
    "lint": "eslint src/"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/eslint-parser": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "babel-loader": "8.1.0",
    "css-loader": "5.0.0",
    "eslint": "7.12.1",
    "html-webpack-plugin": "4.5.0",
    "jest": "26.6.1",
    "jest-cli": "26.6.1",
    "karma": "5.2.3",
    "puppeteer": "5.5.0",
    "jest-image-snapshot": "4.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "4.0.1",
    "webpack": "5.3.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-undef": "warn",
      "no-unused-vars": "warn",
      "valid-jsdoc": [
        "warn",
        {
          "requireReturn": false,
          "requireParamDescription": false,
          "requireReturnDescription": false
        }
      ]
    }
  }
}
